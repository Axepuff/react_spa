{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "semi": ["error", "never"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "quotes": ["error", "single"]
  }
}